{
  "manifest_version": 3,
  "name": "Meaty Fullpage",
  "version": "1.0.8",
  "description": "Lightweight full page screenshot capture",
  "permissions": [
    "activeTab",
    "scripting",
    "tabs"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
